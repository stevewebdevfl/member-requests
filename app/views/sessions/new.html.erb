<!-- app/views/sessions/new.html.erb -->

  <%= render "/users/home" %>

  <h1>Log In</h1>
  
  <div class="login">
  <%= form_tag login_path %>
    <%= label_tag :email %>
    <%= text_field_tag :email %>

    <%= label_tag :password %>
    <%= password_field_tag :password %>

    <%= submit_tag "Log in" %>
  </div>
  <end>
